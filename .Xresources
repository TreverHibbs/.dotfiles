! urxvt
! basic configuration
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt.internalBorder: 14
URxvt.urlLauncher: firefox
URxvt*buffered: true
URxvt*cursorBlink: true
URxvt*font:  xft:inconsolata:size=10:antialias=true
URxvt*borderless: 1
URxvt*scrollBar: false
URxvt*loginShell: true
Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines: 5000
URxvt*termName: rxvt-unicode
URxvt.perl-ext-common: default,matcher
URxvt.matcher.button: 1

! Enable normal copy/paste
URxvt.iso14755:        false
URxvt.iso14755_52:     false
URxvt.keysym.C-M-c:    builtin-string:
URxvt.keysym.C-M-v:    builtin-string:

! Bind Ctrl-Shift-c & Ctrl-Shift-v to copy and paste
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard

! font options
URxvt.font:       xft:Fantasque Sans Mono: size=14: antialiasing=true
Xft*dpi:          96
Xft.antialias:    true
Xft*hinting:      full

! Set depth to make transparency work.
URxvt.depth: 32

! X colors.
! Generated by 'wal'
*foreground:        #c5ccc3
*background:        #0e1c1b
*.foreground:       #c5ccc3
*.background:       #0e1c1b
! emacs*foreground:   #c5ccc3
! emacs*background:   #0e1c1b
! URxvt*foreground:   #c5ccc3
! XTerm*foreground:   #c5ccc3
! UXTerm*foreground:  #c5ccc3
URxvt.background:   [90]#0e1c1b
! XTerm*background:   #0e1c1b
! UXTerm*background:  #0e1c1b
! URxvt*cursorColor:  #c5ccc3
! XTerm*cursorColor:  #c5ccc3
! UXTerm*cursorColor: #c5ccc3
URxvt.borderColor:  [90]#0e1c1b

